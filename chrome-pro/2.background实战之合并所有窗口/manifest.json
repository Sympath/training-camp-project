{
  "name": "浏览器窗口合并",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "合并其它的浏览器窗口到当前的窗口",
  "icons": {
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "img/unit.png",
    "default_title": "窗口合并"
  }
}
